				<button class="btn btn-primary btn-mini" type="button">J</button>
				<button class="btn btn-primary btn-mini" type="button">V</button>
				<button class="btn btn-primary btn-mini" type="button">S</button>
				<button class="btn btn-primary btn-mini" type="button">D</button>
			</div>
		</span>
		
		<!-- Hora termino -->
		<span class="timeend">
			<input id="i" name="i"  />
			<a href="#" id="show" class="btn btn-primary btn-mini">Show</a>
			<a href="#" id="clear" class="btn btn-primary btn-mini">Clear</a>
		</span>
		
		<!-- Java Script reloj termino -->
		<script type="text/javascript">
			$(function(){
			    $('#i').scroller({
			        preset: 'time',
			        theme: 'default',
			        display: 'modal',
			        mode: 'scroller',
			        width: 3
			    });    
			
			    $('#show').click(function(){
			        $('#i').scroller('show'); 
