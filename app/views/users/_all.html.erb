	<h1>Usuarios </h1>
	
	<table class="table table-striped">
	  <tr>
	    <th>Correo</th>
	    <th>Nombre</th>
	    <th>Acciones</th>
	  </tr>
	 
	<% @users.each do |user| %>
  	<% if user.isAdmin == false %>
		  <tr>
		    <td><%= user.email %></td>
		    <td><%= user.name %></td>
		    <td>
		    	<%= link_to 'Editar', edit_user_path(user), :class => 'btn btn-mini', :remote=>true %>
		    	<%= link_to 'Eliminar', user, :confirm => '¿Estás seguro?', :method => :delete, :class => 'btn btn-mini btn-danger', :remote=>true %>
		    </td>	
		  </tr>
	  <%end%>
	<% end %>
	</table>
	
	<br />		


