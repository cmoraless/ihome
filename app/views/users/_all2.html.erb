<%content_for :users do%>
<h1>Usuarios</h1>

<table>
  <tr>
    <th>Correo</th>
    <th>Nombre</th>
    <th>Direccion</th>
    <th>Telefono</th>
    <th>Rut</th>
    <th></th>
    <th></th>
  </tr>
 
<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.name %></td>
    <td><%= user.address %></td>
    <td><%= user.phone %></td>
    <td><%= user.rut %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?',
                                     :method => :delete %></td>
  </tr>
<% end %>
</table>

<%=link_to "Crear nuevo admin", new_admin_path %>
<br />
 
<%end%>