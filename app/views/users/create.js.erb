$("#users").empty();
<% if @user.isAdmin %>
	$("#users").append('<%= j render "shared/flash_messages" %>');
	$("#users").append('<%= j render "users/newAdmin" %>');
<%else%>
	$("#users").append('<%= j render "users/all" %>');
	$("#users").append('<%= link_to "Agregar nuevo usuario", new_no_admin_path, :remote=>true%>');	
<%end%>
