<script type="text/javascript">
	$(function() {
		var correos = [];
		<% for i in 0..@usersAdmin.length-1 %>
			correos.push("<%= @usersAdmin[i][:email]%>");
		<%end%>			
		$( "#emailsIbox" ).autocomplete({
			source: correos
		});
	});
</script>


<div class="ui-widget">
	<%= form_tag(url_for(:controller=>"homeadmin", :action=> "searchIboxes"), :remote=>true)%>
	<%= label_tag 'Ingrese Correo:' %> <%= text_field_tag 'emailsIbox' %>
	<%= submit_tag "Buscar", :class=>"btn btn-primary" %>	
</div>