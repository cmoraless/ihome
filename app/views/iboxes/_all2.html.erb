<!-- PARTIAL PARA LISTAR IBOXES DE UN USUARIO -->
<h3>Iboxs Habilitados</h3>

<table class="table table-striped">
	
  <tr>
 	<th>Id</th>
    <th>Nombre</th>
    <th>IP</th>
    <th>Puerto</th>    
    <th>Acciones</th>
  </tr>

<% @iboxes.each do |ibox| %>
  <tr>
  	<td><%= ibox.id %></td>
  	<td><%= ibox.name %></td>
    <td><%= ibox.ip %></td>
    <td><%= ibox.port %></td>    
    <td><%= link_to 'Editar', edit_ibox_path(ibox), :class => 'btn btn-mini', :remote => true %></td>
  </tr>
<% end %>

</table>





