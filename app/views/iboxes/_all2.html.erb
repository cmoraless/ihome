<!-- PARTIAL PARA LISTAR IBOXES DE UN USUARIO -->
<h1>Iboxs creados</h1>

<table class="table table-striped">
  <tr>
 		<th>Id</th>
    <th>Nombre</th>
    <th>IP</th>
    <th>Puerto</th>
    <th></th>
    <th></th>
  </tr>

<% @iboxes.each do |ibox| %>
  <tr>
  	<td><%= ibox.id %></td>
  	<td><%= ibox.name %></td>
    <td><%= ibox.ip %></td>
    <td><%= ibox.port %></td>
		<td></td>
    <td><%= link_to 'Edit', edit_ibox_path(ibox), :remote => true %></td>
  </tr>
<% end %>
</table>





